  },
  "homepage": "https://github.com/mnrendra/action-jajal-release#readme",
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vercel/ncc": "^0.38.3",
    "clean-package": "^2.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
